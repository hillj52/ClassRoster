package cards;

import java.util.ArrayList;

public class Tester {

	public static void main(String[] args) {
		ArrayList<Integer> al = new ArrayList<Integer>();
		
		al.add(0);
		al.add(1);
		al.add(2);
		
		System.out.println(al.size());
		al.remove(1);
		for(int i=0;i<al.size();i++) {
			System.out.println("index: " + i + " Value: " + al.get(i));
		}
	}

}
