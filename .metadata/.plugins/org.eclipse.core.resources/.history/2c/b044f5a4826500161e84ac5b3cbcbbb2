package ssa;

public class RPNCalculator {
	
	public static void main(String[] args) {
		String s = "123+*";
		char[] problem = new char[] {'2','9','*','3','/','7','+'};
			
		/*
		 * RPNStack constructor can 
		 * be called with either
		 * a String or a char[]
		 */
		RPNStack rpn = new RPNStack(problem);
		System.out.println(rpn);
	}
}
