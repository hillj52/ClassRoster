package ssa;

public class Mainline {
	public static void main(String[] args) {
		Account checking = new Account(500,"My personal checking account");
		Account savings = new Account(1000,"My personal savings account");
		
		System.out.println(checking);
		checking.deposit(200);
		System.out.println(checking);
		
		System.out.println(savings);
	}
}
